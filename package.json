{
    "name": "void-wa",
    "version": "1.0.0",
    "description": "Void is a whatsapp bot created with @open-wa/wa-automate and written in typescript",
    "main": "dist",
    "repository": "https://github.com/Waxaranai/void-wa.git",
    "author": "Waxaranai",
    "license": "GPL-3.0",
    "private": false,
    "scripts": {
        "build": "rimraf dist && tsc",
        "lint": "eslint --ignore-path .gitignore \"**/*.+(ts|js)\"",
        "lint:fix": "eslint --ignore-path .gitignore \"**/*.+(ts|js)\" --fix",
        "start": "node dist",
        "start:dev": "ts-node src/index.ts",
        "test": "yarn lint && yarn build"
    },
    "eslintConfig": {
        "extends": "@hazmi35/eslint-config/typescript/node"
    },
    "devDependencies": {
        "@types/common-tags": "^1.8.0",
        "@types/express": "^4.17.11",
        "@types/mongoose": "^5.10.4",
        "@types/node": "^15.6.0",
        "@types/socket.io": "^2.1.13",
        "@types/superagent": "^4.1.11",
        "@typescript-eslint/eslint-plugin": "^4.23.0",
        "@typescript-eslint/parser": "^4.23.0",
        "dotenv": "^9.0.2",
        "fluent-ffmpeg": "^2.1.2",
        "rimraf": "^3.0.2",
        "sharp": "^0.28.2",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.4"
    },
    "dependencies": {
        "@hazmi35/eslint-config": "hazmi35/eslint-config",
        "@open-wa/wa-automate": "^3.13.2",
        "common-tags": "^1.8.0",
        "date-fns": "^2.21.3",
        "eslint": "^7.26.0",
        "express": "^4.17.1",
        "mongoose": "^5.12.9",
        "socket.io": "^4.1.1",
        "superagent": "^6.1.0",
        "winston": "^3.3.3"
    }
}
