    "@typescript-eslint/parser": "^4.4.0",
    "dotenv": "^8.2.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@hazmi35/eslint-config": "hazmi35/eslint-config",
    "@open-wa/wa-automate": "^2.0.31",
    "common-tags": "^1.8.0",
    "eslint": "^7.11.0",
